<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">There is webpage content</div>
      <div class="panel-body">
      <%= form_for [:admin, @content], url: admin_dashboard_path do |f| %>
        <div class="form-group">
          <%= f.label :logo_icon %>
          <%= f.text_field :logo_icon, class: 'form-control' %>
        </div>
        <div class="form-group">
          <%= f.label :logo_text %>
          <%= f.text_field :logo_text, class: 'form-control' %>
        </div>
        <div class="form-group">
          <%= f.label :header %>
          <%= f.text_field :header, class: 'form-control', required: true %>
        </div>
        <div class="form-group">
          <%= f.label :subheader %>
          <%= f.text_field :subheader, class: 'form-control' %>
        </div>
        <div class="form-group">
          <%= f.label :text %>
          <%= f.text_area :text, class: 'form-control', required: true, rows: 5 %>
        </div>
        <div class="text-center">
          <%= f.submit class: 'btn btn-primary btn-lg' %>
        </div>
      <% end %>

      </div>
    </div>
  </div>
</div>
